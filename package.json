    "mongoose": "^5.7.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@now/node": "^1.0.1",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-inline-dotenv": "^1.3.3",
    "eslint": "^6.5.1",
    "nodemon": "^1.19.3"
  }
