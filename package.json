{
  "name": "nodeapi",
  "version": "1.0.0",
  "description": "my first node api",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir public --source-maps inline",
    "dev": "nodemon --exec babel-node src/index.js",
    "now": "now --public --npm && now alias",
